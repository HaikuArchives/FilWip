;; Opera plugin

[Opera]
TYPE=hierarchical
CHECK INSTALL OF=/boot/home/config/settings/Opera

[Cache]
FOLDER=/boot/home/config/settings/Opera/cache
EXCLUDE FILE=
RECURSE=

[Cookies]
FILE=/boot/home/config/settings/Opera/cookies.dat
EXCLUDE FILE=
RECURSE=

[History]
FILE=/boot/home/config/settings/Opera/global.dat
EXCLUDE FILE=
RECURSE=

[Visited links]
FILE=/boot/home/config/settings/Opera/vlink.dat
EXCLUDE FILE=
RECURSE=
