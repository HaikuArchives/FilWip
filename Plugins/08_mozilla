;; Miscellaneous plugin
;; Some people may have ~/config/settings/mozilla (in small letters not "Mozilla")
;; If u have it that way, replace "/settings/Mozilla" in this file with "/settings/mozilla"

[Mozilla]
TYPE=hierarchical
CHECK INSTALL OF=/boot/home/config/settings/Mozilla

[Cache]
FOLDER=/boot/home/config/settings/Mozilla/default/matirow9.slt/Cache
EXCLUDE FILE=
RECURSE=

[Cookies]
FILE=/boot/home/config/settings/Mozilla/default/matirow9.slt/cookies.txt
EXCLUDE FILE=
RECURSE=

[History]
FILE=/boot/home/config/settings/Mozilla/default/matirow9.slt/history.dat
EXCLUDE FILE=
RECURSE=

[Download list]
FILE=/boot/home/config/settings/Mozilla/default/matirow9.slt/downloads.rdf
EXCLUDE FILE=
RECURSE=
